[/var/log/httpd/{{ item.domain }}-access_log]
file = /var/log/httpd/{{ item.domain }}-access_log
log_group_name = /var/log/httpd/access_log
log_stream_name = {{ item.domain }}-access_log_{instance_id}
datetime_format = %b %d %H:%M:%S

[/var/log/httpd/{{ item.domain }}-error_log]
file = /var/log/httpd/{{ item.domain }}-error_log
log_group_name = /var/log/httpd/error_log
log_stream_name = {{ item.domain }}-error_log_{instance_id}
datetime_format = %b %d %H:%M:%S
